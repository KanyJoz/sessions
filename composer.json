{
    "name": "kanyjoz/sessions",
    "type": "library",
    "description": "A Session handler with PSR-15 middleware",
    "keywords": [
        "slim",
        "session",
        "middleware",
        "helper"
    ],
    "homepage": "https://github.com/KanyJoz/sessions",
    "license": "MIT",
    "require": {
        "php": ">=8.3",
        "psr/http-message": "^2.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "middlewares/utils": "^4.0",
        "nyholm/psr7": "^1.8",
        "phpunit/phpunit": "^11"
    },
    "autoload": {
        "psr-4": {
            "KanyJoz\\Sessions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KanyJoz\\Session\\Test\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
